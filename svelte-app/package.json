{
  "name": "svelte-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv public --cors --single",
    "backend": "json-server --watch db.json --routes routes.json --port 7030",
    "quick": "concurrently \"npm run backend\" \"npm run dev\"",
    "func:start": "cd api && npm start && cd ..",
    "full-stack": "concurrently \"npm run func:start\" \"npm run dev\""
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.0",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.1",
    "concurrently": "^4.0.1",
    "json-server": "^0.15.1",
    "node-sass": "^4.13.1",
    "rollup": "^1.20.0",
    "rollup-plugin-livereload": "^1.0.0",
    "rollup-plugin-postcss": "^2.1.1",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^5.1.2",
    "svelte": "^3.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.10",
    "@fortawesome/free-solid-svg-icons": "^5.6.1",
    "bulma": "^0.8.0",
    "sirv-cli": "^0.4.4",
    "svelte-routing": "^1.4.0"
  }
}
